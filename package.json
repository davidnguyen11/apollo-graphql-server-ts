{
  "name": "apollo-server-ts",
  "version": "0.0.1",
  "description": "A starting point for Node.js express & Apollo GraphQL with TypeScript",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Dzung Nguyen",
  "keywords": [
    "Node.js",
    "Typescript",
    "GraphQL",
    "Express",
    "Apollo"
  ],
  "license": "MIT",
  "scripts": {
    "watch-node": "nodemon dist/server/index.js",
    "start": "node dist/server/index.js",
    "start:dev": "NODE_ENV=development concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "start:debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "start:staging": "NODE_ENV=staging node dist/server/index.js",
    "start:production": "NODE_ENV=production node dist/server/index.js",
    "test": "jest --forceExit --coverage --verbose",
    "build": "rimraf ./dist && npm run build-ts && npm run copy-resources && npm run tslint",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-resources": "ts-node scripts/copyResources.ts",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server/index.js",
    "presequelize": "npm run build",
    "sequelize": "./node_modules/.bin/sequelize"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "async": "^2.6.0",
    "bcrypt-nodejs": "^0.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "graphql": "^0.13.1",
    "graphql-subscriptions": "^0.5.7",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.21.0",
    "graphql-type-json": "^0.1.4",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "merge-graphql-schemas": "^1.4.0",
    "morgan": "^1.9.0",
    "node-fetch": "^2.0.0",
    "nodemailer": "^4.4.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.0-rc.4",
    "request": "^2.83.0",
    "sequelize": "^4.33.4",
    "subscriptions-transport-ws": "^0.9.5"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.18",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.33",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.0.35",
    "@types/express-session": "^1.15.6",
    "@types/graphql": "^0.12.4",
    "@types/graphql-type-json": "^0.1.2",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.91",
    "@types/node": "^9.4.6",
    "@types/nodemailer": "^1.3.32",
    "@types/passport-local": "^1.0.32",
    "@types/request": "^2.0.9",
    "@types/sequelize": "^4.27.7",
    "@types/shelljs": "^0.7.7",
    "@types/supertest": "^2.0.4",
    "@types/ws": "^4.0.1",
    "@types/zen-observable": "^0.5.3",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^21.2.1",
    "nodemon": "^1.13.0",
    "rimraf": "^2.6.2",
    "sequelize-cli": "^4.0.0",
    "shelljs": "^0.7.7",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
